{
    "name": "expressive/expressive-router",
    "description": "Router subcomponent for Expressive",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "expressive",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "homepage": "https://expressive.dev",
    "support": {
        "docs": "https://docs.expressive.dev/expressive-router/",
        "issues": "https://github.com/expressive/expressive-router/issues",
        "source": "https://github.com/expressive/expressive-router",
        "rss": "https://github.com/expressive/expressive-router/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev",
            "dev-develop": "3.2.x-dev"
        },
        "laminas": {
            "config-provider": "Expressive\\Router\\ConfigProvider"
        }
    },
    "require": {
        "php": "^7.1",
        "fig/http-message-util": "^1.1.2",
        "laminas/laminas-zendframework-bridge": "^0.3 || ^1.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~1.0.0",
        "malukenho/docheader": "^0.1.6",
        "phpunit/phpunit": "^7.0.2",
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "expressive/expressive-aurarouter": "^3.0 to use the Aura.Router routing adapter",
        "expressive/expressive-fastroute": "^3.0 to use the FastRoute routing adapter",
        "expressive/expressive-laminasrouter": "^3.0 to use the laminas-router routing adapter"
    },
    "autoload": {
        "psr-4": {
            "Expressive\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExpressiveTest\\Router\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@license-check",
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "license-check": "docheader check src/ test/"
    },
    "replace": {
        "zendframework/zend-expressive-router": "self.version"
    }
}
